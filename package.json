{
  "name": "money-muling-detection",
  "version": "1.0.0",
  "description": "Full-stack Money Muling Detection platform â€” FastAPI + React",
  "private": true,
  "scripts": {
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build && cp -r build ../backend/static",
    "build": "npm run build:frontend",
    "start": "cd backend && uvicorn main:app --host 0.0.0.0 --port ${PORT:-8000}"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": ["fraud-detection", "money-muling", "graph-analysis", "fastapi", "react"],
  "license": "MIT"
}
